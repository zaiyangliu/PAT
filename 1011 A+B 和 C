#include<cstdio>
#include<iostream>
using namespace std;
int main(){
  int T;
  long long  a, b, c;
  int k = 1;
  scanf("%d", &T);
  while(T--){
      scanf("%lld%lld%lld", &a, &b, &c);
      if(a + b > c)
        //printf("Case #%d: true\n", k);
        cout <<"Case #" << k <<": true" << endl;
      else
        //printf("Case #%d: false\n", k);
        cout <<"Case #" << k << ": false" << endl;
      k++;
      
    }
    return 0;
}
